<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id = "mario" class = "com.Launchcode.Moeller.Mario">
    <constructor-arg ref = "printerFactory"/>
</bean>

    <bean id = "printerFactory" class = "com.Launchcode.Moeller.PrinterFactory">
        <constructor-arg ref = "console"/>
        <constructor-arg ref = "saveFile"/>
    </bean>

    <bean id = "console" class = "com.Launchcode.Moeller.Console">

    </bean>



    <bean id = "saveFile" class="com.Launchcode.Moeller.SaveFile">
    </bean>

</beans>
